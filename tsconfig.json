{
    "include": [
        "src/**/*.ts"
    ],
    "compilerOptions": {
        "target": "es2018",
        "module": "esnext",
        "lib": [
            "esnext",
            "DOM"
        ],
        "moduleResolution": "Bundler",
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "resolveJsonModule": true
    },
    
}
